{
  "version": 2,
  "name": "demo-mdxdeck-nowv2",
  "alias": "demo-mdxdeck-nowv2.now.sh",
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    { "src": "^/images/(.*)", "dest": "/images/$1" },
    { "src": "^/snippets/(.*)", "dest": "/snippets/$1" },
    { "src": "index.html", "dest": "/index.html" }
  ]
}
